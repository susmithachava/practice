import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
     
       String s = in.next();
    System.out.println(s.chars().filter(c -> Character.isUpperCase((char)c)).count()+1);
    }
}

